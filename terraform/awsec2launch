terraform file to launch ec2 instance using user accesskey and secret key


terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 4.0"
    }
  }
}

provider "aws" {
  region     = "us-east-1"
  access_key = "AKIAXCQQOE6FB45O564B"
  secret_key = "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"
}

resource "aws_instance" "s1" {
  ami           = "ami-0dfcb1ef8550277af"
  instance_type = "t2.micro"

  tags = {
    Name = "server1"
  }
}
